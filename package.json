{
  "name": "nt-angular-context",
  "version": "1.0.0",
  "description": "angular module to manage devextreme OdataContext and other context utilities",
  "keywords": [
    "angular", "odata", "devextreme", "babel", "bdm", "gdm", "zuk", "nt"
  ],
  "main": "index.js",
  "scripts": {
    "clean": "rimraf .tmp && rimraf dist",
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "\"./node_modules/uglify-js/bin/uglifyjs\" dist/bundles/nt-angular-context.js --screw-ie8 --compress --mangle --comments --output dist/bundles/nt-angular-context.umd.min.js",
    "copy": "cpx './package-dist.json' dist && renamer --find 'package-dist.json' --replace 'package.json' ./dist/*",
    "build": "npm run clean && npm run transpile && npm run package && npm run minify && npm run copy"
  },
  "license": "MIT",
  "homepage": null,
  "devDependencies": {
    "@angular/compiler": "~5",
    "@angular/compiler-cli": "~5",
    "cpx": "~1",
    "renamer": "latest",
    "rimraf": "~2",
    "rollup": "latest",
    "typescript": "~2",
    "uglify-js": "~3"
  },
  "dependencies": {
    "@angular/common": "~5",
    "@angular/core": "~5",
    "rxjs": "~5",
    "zone.js": "latest",
    "moment": "~2",
    "tslint": "~5",
    "@types/node": "~8",
    "devextreme": "~17"
  }
}
