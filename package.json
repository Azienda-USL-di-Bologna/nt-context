{
  "name": "@bds/nt-angular-context",
  "version": "1.0.20",
  "description": "angular module to manage devextreme OdataContext and other context utilities",
  "keywords": [
    "angular",
    "odata",
    "devextreme",
    "babel",
    "bdm",
    "gdm",
    "zuk",
    "nt"
  ],
  "main": "index.js",
  "scripts": {
    "clean": "rimraf .tmp && rimraf dist",
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "node node_modules\\uglify-js\\bin\\uglifyjs dist\\bundles\\nt-angular-context.umd.js --screw-ie8 --compress --mangle --comments --output dist\\bundles\\nt-angular-context.umd.min.js",
    "copy": "copy package-dist.json dist\\package.json",
    "build": "npm run clean && npm run transpile && npm run package && npm run minify && npm run copy"
  },
  "license": "MIT",
  "homepage": null,
  "devDependencies": {
    "@angular/compiler": "~4",
    "@angular/compiler-cli": "~4",
    "cpx": "~1",
    "renamer": "latest",
    "rimraf": "~2",
    "rollup": "latest",
    "typescript": "~2.4.2",
    "uglify-js": "~3"
  },
  "dependencies": {
    "@angular/common": "~4",
    "@angular/core": "~4",
    "@angular/router": "~4",
    "rxjs": "~5",
    "zone.js": "^0.8.4",
    "moment": "~2",
    "tslint": "~5.3.2",
    "@types/node": "~6.0.60",
    "devextreme": "~17"
  }
